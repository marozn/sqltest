<IfModule mod_rewrite.c>
    RewriteEngine On
    
    RewriteEngine On
    RewriteRule ^sitemap\.xml$ sitemap.php [L]

    RewriteCond %{HTTP_USER_AGENT} (android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge\ |maemo|midp|mmp|netfront|opera\ m(ob|in)i|palm(\ os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows\ (ce|phone)|xda|xiino [NC]
    RewriteCond %{HTTP_HOST}   !^m.sqltest.online
    RewriteCond %{HTTP_HOST}   !^localhost
    RewriteRule (.*)$ https://m.sqltest.online/$1 [L,R=302]

    RewriteCond %{HTTP_USER_AGENT} "!(android|blackberry|googlebot-mobile|iemobile|ipad|iphone|ipod|opera mobile|mobile safari|palmos|webos)" [NC]
    RewriteCond %{HTTP_HOST}   !^sqltest.online
    RewriteCond %{HTTP_HOST}   !^localhost
    RewriteRule (.*)$ https://sqltest.online/$1 [L,R=302]
</IfModule>